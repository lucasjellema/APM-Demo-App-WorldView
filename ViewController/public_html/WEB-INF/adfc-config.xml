<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="WorldMap">
    <page>/WorldMap.jsf</page>
  </view>
  <view id="main">
    <page>/main.jsf</page>
  </view>
 
  <managed-bean id="__1">
    <managed-bean-name>mainBean</managed-bean-name>
    <managed-bean-class>nl.amis.world.view.beans.MainBean</managed-bean-class>
    <managed-bean-scope>view</managed-bean-scope>
  </managed-bean>
  <!-- SuppressWarning: oracle.ide.xml.validation-error -->
  <managed-bean id="__2">
    <managed-bean-name>newsBean</managed-bean-name>
    <managed-bean-class>nl.amis.world.view.beans.NewsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property id="__4">
      <property-name>rssFeedUrl</property-name>
      <property-class>java.lang.String</property-class>
      <value>http://rss.cnn.com/rss/cnn_topstories.rss</value>
    </managed-property>
  </managed-bean>
</adfc-config>
